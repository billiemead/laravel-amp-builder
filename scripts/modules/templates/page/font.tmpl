<md-dialog rc-drag="md-toolbar" aria-label=" {{('fontpopup.title')| lang }}">
	<form ng-cloak ng-submit="ok()">
    <md-toolbar>
      <div class="md-toolbar-tools">
        <h2> {{('fontpopup.title')| lang }} </h2>
		
        <span flex></span>
        <md-button class="md-icon-button" ng-click="$dismiss()">
          <i class="fa fa-times" aria-label="Close dialog"></i>
        </md-button>
      </div>
    </md-toolbar>
	<md-dialog-content>
		<div class="md-dialog-content">
			<md-button class="md-raised md-primary" ng-click="close('google')">{{('fontpopup.google_font')| lang }}</md-button>
		</div>
	</md-dialog-content>
	<md-dialog-actions layout="row">
		
    <span flex></span>
		
      <md-button ng-click="$dismiss()">
         {{('builder.cancel')| lang }} 
      </md-button>
    </md-dialog-actions>
  </form>
</md-dialog>