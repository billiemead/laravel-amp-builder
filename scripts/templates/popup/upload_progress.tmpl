<md-dialog rc-drag="md-toolbar" aria-label="" flex="66" class="md-code-popup">
  <form ng-cloak ng-submit="ok()">
    <md-toolbar>
      <div class="md-toolbar-tools">
        <h2>Uploading</h2>
        <span flex></span>
        <md-button class="md-icon-button" ng-click="$dismiss()">
          <i class="fa fa-times" aria-label="Close dialog"></i>
        </md-button>
      </div>
    </md-toolbar>

    <md-dialog-content>
<div class="">
		<div class="block">
			<div class="progress_list">
				<div ng-repeat="file in files">
					<div class="text-center"><h5>{{file.name}}</h5></div>
					<md-progress-linear value="{{file.percent}}">{{file.percent }}%</md-progress-linear>
				</div>
			</ul>
		</div>
	</div>
</div>
</md-dialog-content>

