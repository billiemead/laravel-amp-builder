{
  "name": "ampbuilder",
  "version": "1.0.0",
  "description": "AMPBuilder",
  "scripts": {
    "dev": "set NODE_ENV=development & webpack --watch --progress --hide-modules",
    "posttest": "nyc report --reporter=html",
    "production": "set NODE_ENV=production & webpack --progress --hide-modules",
    "test": "nyc ava --verbose --serial",
    "webpack": "set NODE_ENV=development&  webpack --progress --hide-modules",
    "bundle": "webpack  --progress --colors --watch --config webpack.config-main.js",
    "cost-of-modules": "webpack  cost-of-modules",
    "bundle-min": "webpack  --progress --colors --watch --config webpack-min.config.js",
    "bundle_front": "webpack  --progress --colors --watch --config webpack_front.config.js",
    "bundle-quill": "webpack  --progress --colors --watch --config webpack.config-quill.js",
    "bundle-medium": "webpack  --progress --colors --watch --config webpack.config-medium.js",
    "bundle-scss": "webpack  --progress --colors --watch --config webpack.mix.js",
    "build-css": "node-sass --include-path scss scss/admin/style.scss   public/assets/css/admin/style.css --output-style compressed",
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
    "build-builder-css": "node-sass --include-path scss scss/builder/style.scss   public/assets/css/builder/style.css --output-style compressed",
    "build-ebuilder-css": "node-sass --include-path scss scss/builder/edit_page.scss   public/assets/css/builder/edit_page.css --output-style compressed",
    "watch-ebuilder-css": "nodemon -e scss -x \"npm run build-ebuilder-css\"",
    "watch-builder-css": "nodemon -e scss -x \"npm run build-builder-css\"",
    "build-app-css": "node-sass --include-path scss scss/app/style.scss   public/assets/css/app/style.css --output-style compressed",
    "watch-app-css": "nodemon -e scss -x \"npm run build-app-css\""
  },
  "keywords": [],
  "author": "STSBuilder",
  "license": "ISC",
  "main": "webpack.config.js",
  "dependencies": {
    "@uirouter/angularjs": "^1.0.24",
    "angular-animate": "^1.7.9",
    "angular-aria": "^1.7.9",
    "angular-bootstrap-contextmenu": "^0.9.9",
    "angular-datatables": "^0.6.4",
    "angular-intro.js": "^3.4.0",
    "angular-lodash": "^0.1.1",
    "angular-material": "^1.1.21",
    "angular-messages": "^1.7.9",
    "angular-sanitize": "^1.7.9",
    "angular-tree-control": "^0.2.30",
    "cross-storage": "^1.0.0",
    "datatables.net": "^1.10.20",
    "datatables.net-responsive": "^2.2.3",
    "del": "^4.1.1",
    "extends-classes": "^1.0.5",
    "google-fonts-complete": "^1.2.3",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-uglify": "^3.0.2",
    "jquery-modal": "^0.9.2",
    "lodash.throttle": "^4.1.1",
    "md-steppers": "^0.3.4",
    "medium-editor": "^5.23.3",
    "mixwith": "^0.1.1",
    "moment": "^2.24.0",
    "ng-quill": "^4.5.0",
    "path": "^0.12.7",
    "php-parser": "^2.2.0",
    "plupload": "^2.3.6",
    "quill": "^1.3.7",
    "rangy": "^1.3.0",
    "raw-loader": "^1.0.0",
    "restangular": "^1.6.1",
    "right-click-menu": "0.0.4",
    "split.js": "^1.5.11",
    "ts-loader": "^5.4.5",
    "typescript": "^3.7.5",
    "vanilla-lazyload": "^11.0.6"
  },
  "devDependencies": {
    "angular": "^1.7.9",
    "angular-ui-bootstrap": "^2.5.6",
    "angular-ui-router": "^0.2.18",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "clone": "^2.1.2",
    "css-loader": "^3.4.2",
    "file-loader": "^0.11.2",
    "fitty": "^2.2.6",
    "html-loader": "^0.5.5",
    "jquery": "^3.4.1",
    "jquery-json": "^2.6.0",
    "jquery-migrate": "^1.4.0",
    "jquery-ui": "^1.12.1",
    "jquery-ui-bundle": "^1.12.1",
    "json-loader": "^0.5.7",
    "mobile-angular-ui": "^1.3.4",
    "mustache": "^2.3.2",
    "ng-annotate": "^1.2.2",
    "ng-annotate-loader": "^0.6.1",
    "ngstorage": "^0.3.11",
    "ngtemplate-loader": "^2.0.1",
