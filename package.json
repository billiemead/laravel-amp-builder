    "resolve-url-loader": "^2.3.1",
    "sass-loader": "^7.3.1",
    "style-loader": "^0.18.2",
    "ui-router-extras": "^0.1.3",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "directories": {
    "test": "tests"
  }
}
